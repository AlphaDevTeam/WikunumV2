<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity DocumentHistory.
    -->
    <changeSet id="20191208170745-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="type_id"
                                 baseTableName="document_history"
                                 constraintName="fk_document_history_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="document_history"
                                 constraintName="fk_document_history_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ex_user"/>

        <addForeignKeyConstraint baseColumnNames="change_log_id"
                                 baseTableName="document_history"
                                 constraintName="fk_document_history_change_log_id"
                                 referencedColumnNames="id"
                                 referencedTableName="change_log"/>

    </changeSet>
</databaseChangeLog>
