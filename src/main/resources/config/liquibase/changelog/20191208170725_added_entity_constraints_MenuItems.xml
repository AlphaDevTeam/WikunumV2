<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity MenuItems.
    -->
    <changeSet id="20191208170725-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="history_id"
                                 baseTableName="menu_items"
                                 constraintName="fk_menu_items_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_history"/>

        <addForeignKeyConstraint baseColumnNames="user_permission_id"
                                 baseTableName="menu_items"
                                 constraintName="fk_menu_items_user_permission_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_permissions"/>

    </changeSet>
</databaseChangeLog>
